{
    "name": "onurmutlu/parasut-laravel8",
    "description": "This is my package Parasut",
    "keywords": [
        "onurmutlu",
        "parasut-laravel8"
    ],
    "homepage": "https://github.com/onurmutlu/parasut-laravel8",
    "license": "MIT",
    "authors": [
        {
            "name": "SiyahKare",
            "email": "siyahkare@siyhkare.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^5.8.33 || ^6.0 || ^7.0",
        "illuminate/http": "^5.8.33 || ^6.0 || ^7.0",
        "illuminate/support": "^5.8.33 || ^6.0 || ^7.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Onurmutlu\\Parasut\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onurmutlu\\Parasut\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
